#!/bin/bash

GREEN='\033[0;32m'
NO_COLOR='\033[0m'
rootDir="$( dirname "$0" )/../.."

(
  cd "$rootDir/react" || exit
  cp build/static/js/main*.js ../app/assets/javascripts/folio/console/react/main.js
  cp build/static/css/main*.css ../app/assets/stylesheets/folio/console/react/main.scss

  echo -e "${GREEN}Copied build to rails assets folder. $NO_COLOR"
)
